{
    "name": "@repo/backend",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "imports": {
        "#src/*": "./dist/src/*"
    },
    "exports": {
        ".": {
            "default": "./dist/src/index.js",
            "types": "./dist/src/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
